<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Web_Inlupp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header class="header">
        <nav class="navbar navbar-expand-lg navbar-light bg-white py-3 py-lg-2">
            <div class="container">
                <a class="navbar-brand py-3 d-flex align-items-center" href="index.html"><img src="img/logo.svg" alt="" width="30"><span class="text-uppercase text-small font-weight-bold text-dark ml-2 mb-0">vehiculum</span></a>
                <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <!-- Navbar link--><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <!-- Navbar link--><a class="nav-link text-dark" asp-area="" asp-controller="Shop" asp-action="ShopIndex">Shop</a>
                        </li>
                        <li class="nav-item">
                            <!-- Navbar link--><a class="nav-link text-dark" asp-area="" asp-controller="Administration" asp-action="Create">Create</a>
                        </li>
                        <li class="nav-item ml-lg-2 py-2 py-lg-0">
                            <partial name="_LoginPartial" />
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="border-top footer text-muted" style="background: #111;">
        <div class="container py-4">
            <div class="row py-5">
            </div>
        </div>
        <div class="copyrights py-4" style="background: #0e0e0e">
            <div class="container">
                <div class="row text-center">
                    <div class="col-md-6 text-lg-left mb-2 mb-md-0">
                        <p class="mb-0 text-muted mb-0 text-small">&copy; 2020 All rights reserved.</p>
                    </div>
                    <div class="col-md-6 col-sm-6 text-md-right">
                        <p class="mb-0 text-muted mb-0 text-small">
                            copy; 2021 - Web_Inlupp - Template designed by <a class="reset-anchor text-primary" target="_blank" href="https://bootstraptemple.com/p/listings">Bootstrap Temple</a>
                            .
                        </p>
                        <!-- If you want to remove the backlink, please purchase the Attribution-Free License.-->
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>